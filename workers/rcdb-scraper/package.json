{
  "name": "@logalise/rcdb-scraper",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "scrape:coasters": "tsx ./index.ts -e coaster --only-existing",
    "scrape:coasters:defunct": "tsx ./index.ts -e coaster --only-defunct",
