  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "cli-progress": "^3.12.0",
    "tsx": "^4.19.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^20.17.10",
    "@types/yargs": "^17.0.33"
  },
  "engines": {
    "node": ">=20"
  }
}
